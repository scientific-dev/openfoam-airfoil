FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    freestream
    {
        type            freestreamPressure;
        freestreamValue $internalField;
    }

    airfoil
    {
        type            zeroGradient;
    }
    
    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
} 

// ************************************************************************* //
