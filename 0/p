FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pOut            1e5;

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform $pOut;

boundaryField
{
    outlet
    {
        type            waveTransmissive;
        field           p;
        gamma           1.4;
        fieldInf        $pOut;
        lInf            5;
        value           uniform $pOut;
    }

    inlet
    {
        type            freestreamPressure;
        freestreamValue uniform $pOut;
    }

    airfoil
    {
        type            zeroGradient;
    }

    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }

    #includeEtc "caseDicts/setConstraintTypes"
} 

// ************************************************************************* //
